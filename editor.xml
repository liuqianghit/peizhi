<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="FONT_FAMILY" value="Consolas" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\btagwjz\b.*" />
    <pattern pattern="\becf\b.*" />
    <pattern pattern="\b\?" />
    <filter name="wjz">
      <pattern index="2" />
    </filter>
    <filter name="engine call flow">
      <pattern index="3" />
    </filter>
    <filter name="??">
      <pattern index="4" />
    </filter>
  </component>
</application>